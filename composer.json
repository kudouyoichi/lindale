{
    "name": "lindelin/lindale",
    "description": "Project Manager",
    "keywords": ["laravel", "Lindale", "application", "lindelin", "Project", "Project Manager"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Lindelea",
            "email": "kgo@lindelin.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "aws/aws-sdk-php": "^3.24",
        "barryvdh/laravel-ide-helper": "^2.3",
        "consoletvs/charts": "4.*",
        "doctrine/dbal": "^2.5",
        "erusev/parsedown-extra": "^0.7.1",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "laravel/dusk": "^1.1",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^3.0",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "lindelea/laravel-aws-elb-support": "^1.3",
        "rap2hpoutre/laravel-log-viewer": "^0.8.0",
        "spatie/laravel-backup": "^5.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "laravel/homestead": "^5.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Tools/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ]
}
