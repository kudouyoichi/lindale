{
    "name": "lindelin/lindale",
    "description": "Project Manager",
    "keywords": ["laravel", "Lindale", "application", "lindelin", "Project", "Project Manager"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Lindelea",
            "email": "kgo@lindelin.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "erusev/parsedown-extra": "^0.7.1",
        "laravel/passport": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "doctrine/dbal": "^2.5",
        "spatie/laravel-backup": "^4.6",
        "consoletvs/charts": "4.*",
        "rap2hpoutre/laravel-log-viewer": "^0.8.0",
        "barryvdh/laravel-ide-helper": "^2.3",
        "intervention/image": "^2.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "garygreen/pretty-routes": "^0.0.6",
        "aws/aws-sdk-php": "^3.24",
        "lindelea/laravel-aws-elb-support": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "laravel/homestead": "^5.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ]
}
