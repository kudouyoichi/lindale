{
    "name": "lindelin/lindale",
    "description": "Project Manager",
    "keywords": ["laravel", "Lindale", "application", "lindelin", "Project", "Project Manager"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Lindelea",
            "email": "kgo@lindelin.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "aws/aws-sdk-php": "^3.24",
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "^2.5",
        "erusev/parsedown-extra": "^0.7.1",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "kg-bot/laravel-localization-to-vue": "^1.6",
        "laravel/framework": "5.7.*",
        "laravel/passport": "~5.0",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-backup": "5.10.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/homestead": "^8.2",
        "mockery/mockery": "0.9.*",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "~7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Tools/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
